<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
<extension point="org.eclipse.debug.core.launchConfigurationTypes">
	    <launchConfigurationType
           delegate="org.somox.ejbmox.ui.EJBmoxModelAnalyzerConfigurationDelegate"
           id="org.somox.ejbmox.analyzer.ejbmodelanalyzer.runconfig.LaunchEJBModelAnalyzer"
           modes="run, debug"
           name="Launch EJBmox"
           public="true">        
	    </launchConfigurationType>
   </extension>
   <extension point="org.eclipse.debug.ui.launchConfigurationTabGroups">
	    <launchConfigurationTabGroup
	        type="org.somox.ejbmox.analyzer.ejbmodelanalyzer.runconfig.LaunchEJBModelAnalyzer"
	        class="org.somox.ejbmox.ui.EJBmoxModelAnalyzerTabGroup"
	        id="org.somox.analyzer.ejbmoxanalyzer.ui.launchConfigurationTabGroupAnalyzer">
	    </launchConfigurationTabGroup>
	</extension>
	<extension point="org.eclipse.debug.ui.launchConfigurationTypeImages">
		<launchConfigurationTypeImage
	        icon="icons/ejbmox_16x16.gif"
	        configTypeID="org.somox.ejbmox.analyzer.ejbmodelanalyzer.runconfig.LaunchEJBModelAnalyzer"
	        id="org.somox.ejbmox.ui.runconfig.LaunchSimpleModelAnalyzerImage">
	    </launchConfigurationTypeImage>
	</extension>
</plugin>
